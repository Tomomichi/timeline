{
  "version": 2,
  "build": {
    "env": {
      "BUILDING_FOR_NOW": "true"
    }
  },
  "builds": [
    { "src": "app/package.json", "use": "@now/next" },
    { "src": "web/package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/app/_next(.*)", "dest": "app/_next$1" },
    { "src": "/app(.*)", "dest": "app$1" },
    { "src": "(.*)", "dest": "web$1" }
  ]
}
